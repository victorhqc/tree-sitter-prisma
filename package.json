{
  "name": "tree-sitter-prisma",
  "version": "1.5.1",
  "description": "Prisma Grammar with Tree Sitter",
  "keywords": [
    "tree-sitter",
    "prisma",
    "grammar"
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "homepage": "https://github.com/victorhqc/tree-sitter-prisma#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorhqc/tree-sitter-prisma.git"
  },
  "license": "MIT",
  "author": {
    "name": "Victor Quiroz Castro",
    "email": "git@victorhqc.com"
  },
  "contributors": [
    {
      "name": "Ricardo Madriz",
      "email": "richin13@gmail.com"
    }
  ],
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "npm run generate && npm run test:grammar",
    "generate": "tree-sitter generate",
    "test:grammar": "tree-sitter test",
    "test:bindings": "node --test bindings/node/*_test.js"
  },
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  }
}
